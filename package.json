{
  "name": "image-search",
  "private": true,
  "version": "0.0.0",
  "description": "Free Code Camp - Image Search Abstraction Layer",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "babel": "rimraf dist && babel src -d dist",
    "start": "node dist",
    "start:dev": "babel-node src",
    "knex": "babel-node node_modules/.bin/knex --",
    "db:make": "npm run -s knex migrate:make --",
    "db:up": "npm run -s knex migrate:latest",
    "db:down": "npm run -s knex migrate:rollback",
    "db:cycle": "npm run -s db:down && npm run -s db:up",
    "watch": "npm-watch"
  },
  "watch": {
    "start:dev": {
      "patterns": [
        "src"
      ]
    },
    "babel": {
      "patterns": [
        "src"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "eslint": "2.9.0",
    "npm-watch": "0.1.4",
    "rimraf": "2.5.2"
  },
  "dependencies": {
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-1": "6.5.0",
    "express": "4.13.4",
    "express-async-wrap": "1.0.0",
    "google-images": "1.0.0",
    "knex": "0.10.0",
    "pg": "4.5.5"
  }
}
